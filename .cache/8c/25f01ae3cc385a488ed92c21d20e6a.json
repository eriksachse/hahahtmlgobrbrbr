ansform: translate(-50%, -50%) scale(1);\n  }\n  100% {\n    opacity: 0;\n    display: none;\n  }\n}\n#info-title {\n  color: blue;\n  background-color: rgba(255, 255, 255, 0.4);\n  border-radius: 50%;\n  text-align: center;\n  display: inline-block;\n  width: 30px;\n  height: 30px;\n  top: 0;\n  left: 0;\n  font-size: 24px;\n  cursor: pointer;\n  z-index: 12;\n  position: absolute;\n  animation: shake 5s ease infinite;\n}\n#info-title:hover {\n  background-color: rgba(255, 255, 255, 0.8);\n}\n@keyframes shake {\n  0% {\n    transform: rotate(0deg);\n  }\n  10% {\n    transform: rotate(-50deg);\n  }\n  12% {\n    transform: rotate(20deg);\n  }\n  14% {\n    transform: rotate(-10deg);\n  }\n  20% {\n    transform: rotate(0deg);\n  }\n  100% {\n    transform: rotate(0deg);\n  }\n}\n.info-content {\n  background-color: rgba(255, 255, 255, 0.4);\n\n  box-sizing: border-box;\n  /* border: 1px solid black; */\n  padding: 0 12px;\n  padding-top: 0px;\n  font-size: 24px;\n  height: 0vh;\n  transition: transform 0.5s linear;\n  /* padding-top: 120px; */\n  height: 400px;\n  width: 500px;\n  position: absolute;\n  z-index: 11;\n  overflow: scroll;\n  transform: translateY(-100vh);\n  margin: 12px 64px;\n  border-radius: 24px;\n}\n.visible {\n  overflow: scroll;\n  transform: translateY(0vh);\n}\n\n.images {\n  /* margin-top: 50%; */\n  /* display: grid;\n  grid-template-columns: 50% 50%; */\n  width: 100vw;\n  height: 100vh;\n  position: fixed;\n  perspective: 600px;\n}\n.img-item {\n  width: 200px;\n  height: 200px;\n  position: absolute;\n  top: 0;\n  left: 0;\n  border-radius: 50%;\n  overflow: hidden;\n  /* background: white;*/\n  transform-style: preserve-3d;\n  box-sizing: border-box;\n}\n.img-item:hover {\n  border: 1px solid gold;\n}\n.image-item {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  transform: rotateZ(0deg) rotateX(0deg);\n  position: absolute;\n  top: 0;\n}\n.name {\n  height: 20px;\n  padding: 20px;\n  border-radius: 12px;\n  position: absolute;\n  top: calc(0% - 42px);\n  left: 50%;\n  margin-right: -50%;\n  transform: translate(-50%, -50%) rotate(180deg);\n  background: rgba(255, 255, 255, 0.4);\n  transition: top 0.6s ease, transform 0.6s ease;\n  z-index: 12;\n}\n.name-active {\n  top: calc(50% - 12px);\n  transform: translate(-50%, -50%) rotate(0deg);\n}\n#vid {\n  position: relative;\n  object-fit: cover;\n  width: 100%;\n  height: 100%;\n  background: transparent\n    url(\"../imgs/Bildschirmfoto 2021-02-16 um 10.04.19.png\") no-repeat;\n  background-size: cover;\n}\nvideo {\n  display: block;\n}\n#vid-rap {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  position: fixed;\n  top: 0;\n  right: 0;\n  z-index: -100;\n}\n"},"lineCount":null}},"error":null,"hash":"36245c393de5fb2d43d88f5e99b65938","cacheData":{"env":{}}}